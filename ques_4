a = [5,7,9,3,2,1,4,2,6,3,0,9,8]
b = []
for x in a:
    if(x < 5):
        print(x)
        b.append(x)

print("Second list: ",b)

n = int(input("Enter a number: "))
flag = 0
for x in b:
    if x == n:
        flag = 1
        break

if flag == 1:
    print("Present")
elif flag == 0:
    print("Not present")
else:
    print("Wrong input")